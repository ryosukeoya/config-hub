{
  "workbench.colorTheme": "Material Theme Ocean",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.printWidth": 200,
  "diffEditor.ignoreTrimWhitespace": false,
  "prettier.singleQuote": true,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "editor.formatOnPaste": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
  },
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "prettier.jsxSingleQuote": true,
  "files.autoSave": "afterDelay",
  "commands": [
    {
      "match": ".scss",
      "isasync": true,
      "cmd": "npm run lint:s"
    }
  ],
  "[php]": {
    "editor.defaultformatter": "esbenp.prettier-vscode"
  },
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": ".php$",
        "cmd": "prettier ${file} --write"
      }
    ]
  },
  "php-cs-fixer.executablepath": "${extensionpath}/php-cs-fixer.phar",
  "php-cs-fixer.lastdownload": 1699252189212,
  "[blade]": {
    "editor.defaultformatter": "onecentlin.laravel-blade",
    "editor.formatonsave": true
  },
  "blade.format.enable": true,
  "[python]": {
    "editor.defaultformatter": "ms-python.black-formatter",
    "editor.formatonsave": true,
    "editor.formatontype": true
  },
  "[ruby]": {
    "editor.defaultformatter": "svipas.prettier-plus"
  },
  "[java]": {
    "editor.defaultformatter": "redhat.java"
  },
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  // "vim.vimrc.enable": true,
  // "vim.vimrc.path": "/etc/vim/vimrc",
  "vim.useSystemClipboard": true,
  // "vim.enableneovim": true,
  // "vim.neovimpath": "/usr/local/bin/nvim,
  // "vscode-neovim.neovimInitVimPaths.linux": "/home/ryo/.config/nvim/init.vim",
  // "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "prettier.singlequote": true,
  "editor.autoclosingbrackets": "always",
  "editor.defaultformatter": "esbenp.prettier-vscode",
  "editor.formatonpaste": true,
  "security.workspace.trust.untrustedfiles": "newwindow",
  "javascript.preferences.quotestyle": "single",
  "typescript.preferences.quotestyle": "single",
  "prettier.jsxsinglequote": true,
  "[prisma]": {
    "editor.defaultformatter": "prisma.prisma"
  },
  "[shellscript]": {
    "editor.defaultformatter": "foxundermoon.shell-format",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[dockerfile]": {
    "editor.defaultformatter": "ms-azuretools.vscode-docker"
  },
  "[markdown]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": "explicit"
    },
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "markdownlint.config": {
    "MD024": false,
    "MD036": false,
    "MD041": false
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "security.workspace.trust.enabled": false,
  "livesasscompile.settings.formats": [
    {
      "format": "expanded",
      "extensionname": ".css",
      "savepath": "/sol/contents/it/lp/engineer_001/css"
    }
  ],
  "easysass.formats": [
    {
      "format": "expanded",
      "extension": ".css"
    },
    {
      "format": "compressed",
      "extension": ".min.css"
    }
  ],
  "explorer.confirmDragAndDrop": false,
  "markdown-preview-enhanced.previewTheme": "github-light.css",
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "markdown-preview-github-styles.colorTheme": "light",
  "editor.quickSuggestions": {
    "strings": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "css.customData": ["./.vscode/custom.css-data.json"],
  "ruby.intellisense": "rubyLocate",
  "githubPullRequests.pullBranch": "never",
  "githubPullRequests.fileListLayout": "flat",
  "svelte.enable-ts-plugin": true,
  "namespaceResolver.sortAlphabetically": true,
  "editor.tabSize": 2,
  "git.confirmSync": false,
  "intelephense.diagnostics.undefinedTypes": false,
  "php.codeLens.enabled": false,
  "gitlens.views.commits.showBranchComparison": false,
  "gitlens.views.branches.files.layout": "list",
  "gitlens.views.commits.files.layout": "list",
  "gitlens.views.remotes.files.layout": "list",
  "markdown-mermaid.darkModeTheme": "base",
  "trailing-spaces.syntaxIgnore": ["markdown"],
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "Recent Issues",
      "query": "state:open repo:${owner}/${repository} sort:updated-desc"
    }
  ],
  "phpstan.timeout": 20000,
  "cSpell.ignoreWords": ["ryosuke", "neovim", "nvim", "protoc"],
  "todohighlight.isEnable": true,
  "todohighlight.isCaseSensitive": true,
  // "todohighlight.keywords": [
  //   "TODO",
  //   {
  //     "color": "#fff",
  //     "backgroundColor": "#fff41a",
  //   }
  // ],
  "todo-tree.filtering.excludeGlobs": ["**/node_modules/*"],
  "todo-tree.general.tags": [
    "TODO",
    "REMOVE_ME",
    "ASK_TO",
    "BUG",
    "HACK",
    "FIXME",
    "ASK_TO"
    "MUST",
    "ATTENTION",
    "XXX",
    "[ ]",
    "[x]",
  ],
  "highlight.regexes": {
    "((TODO:))": {
      "decorations": [
        {
          "color": "#fff41a"
        }
      ]
    },
    "((REMOVE_ME))": {
      "decorations": [
        {
          "color": "#ebd534"
        }
      ]
    },
    "((ASK_TO:))": {
      "decorations": [
        {
          "color": "#a1dc3b"
        }
      ]
    },
    "((MUST:))": {
      "decorations": [
        {
          "color": "#ff0000"
        }
      ]
    },
    "((ATTENTION:))": {
      "decorations": [
        {
          "color": "#ff0000"
        }
      ]
    },
    "((FIXME:))": {
      "decorations": [
        {
          "color": "#fff2f2",
          "backgroundColor": "#ff0000"
        }
      ]
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *TODO(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "filterFileRegex": ".*(?<!CHANGELOG.md)$",
      "decorations": [
        {
          "overviewRulerColor": "#ffcc00",
          "backgroundColor": "#ffcc00",
          "color": "#1f1f1f",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "#ffcc00",
          "color": "#1f1f1f"
        }
      ]
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:FIXME|FIX|BUG|UGLY|DEBUG|HACK)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "filterFileRegex": ".*(?<!(CHANGELOG.md|package.json))$",
      "decorations": [
        {
          "overviewRulerColor": "#cc0000",
          "backgroundColor": "#cc0000",
          "color": "#1f1f1f",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "#cc0000",
          "color": "#1f1f1f"
        }
      ]
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:REVIEW|OPTIMIZE|TSC)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "filterFileRegex": ".*(?<!CHANGELOG.md)$",
      "decorations": [
        {
          "overviewRulerColor": "#00ccff",
          "backgroundColor": "#00ccff",
          "color": "#1f1f1f",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "#00ccff",
          "color": "#1f1f1f"
        }
      ]
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:IDEA)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "filterFileRegex": ".*(?<!CHANGELOG.md)$",
      "decorations": [
        {
          "overviewRulerColor": "#cc00cc",
          "backgroundColor": "#cc00cc",
          "color": "#1f1f1f",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "#cc00cc",
          "color": "#1f1f1f"
        }
      ]
    }
  },
  "terminal.integrated.sendKeybindingsToShell": false,
  "remote.autoForwardPortsSource": "hybrid",
  "editor.stickyScroll.enabled": false,
  "workbench.tree.enableStickyScroll": true,
  "terminal.integrated.stickyScroll.maxLineCount": 5,
  "editor.stickyScroll.defaultModel": "outlineModel",
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#63c3dd",
    "activityBar.activeBorder": "#63c3dd",
    "panel.border": "#bbbbbb0d",
    "panelTitle.activeBorder": "#63c3dd",
    "terminal.findMatchBackground": "#2b2700ec",
    "terminal.findMatchBorder": "#7b700dec",
    "editorStickyScroll.background": "#0a1a2ec1"
  },
  "editor.tokenColorCustomizations": {
    "comments": "#9398a07a"
  },
  "hediet.vscode-drawio.resizeImages": null
}
