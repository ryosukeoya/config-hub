{
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[go]": {
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "files.autoSave": "onFocusChange"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[makefile]": {
    "editor.indentSize": "tabSize",
    "editor.insertSpaces": false,
    "editor.tabSize": 8
  },
  "[markdown]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": "explicit"
    },
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "files.insertFinalNewline": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[mk]": {
    "editor.indentSize": "tabSize",
    "editor.insertSpaces": false,
    "editor.tabSize": 8
  },
  "[php]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": false,
    "editor.formatOnType": true
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "files.autoSave": "afterDelay"
  },
  "[yaml]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.autoIndent": "keep",
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": true
    },
    "editor.tabSize": 2
  },
  "go.coverageDecorator": {
    "type": "gutter"
  },
  "go.coverOnSave": true,
  "go.coverOnSingleTest": true,
  "go.coverOnTestPackage": true,
  "cSpell.ignoreWords": [
    "ryosuke",
    "neovim",
    "nvim",
    "protoc"
  ],
  "css.customData": [
    "./.vscode/custom.css-data.json"
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.defaultFormatter": "mtxr.sqltools",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.stickyScroll.defaultModel": "outlineModel",
  "editor.stickyScroll.enabled": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "comments": "#9398a07a",
    "textMateRules": [
      {
        "scope": "markup.strikethrough",
        "settings": {
          "fontStyle": "strikethrough"
        }
      }
    ]
  },
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "cmd": "goimports -w ${file}",
        "match": ".go$"
      }
    ]
  },
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.envrc.*": "dotenv",
    "*.mk": "makefile",
    ".env*": "dotenv"
  },
  "files.autoSave": "afterDelay",
  "git.confirmSync": false,
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "Recent Issues",
      "query": "state:open repo:${owner}/${repository} sort:updated-desc"
    }
  ],
  "githubPullRequests.fileListLayout": "flat",
  "githubPullRequests.pullBranch": "never",
  "gitlens.ai.experimental.model": "openai:gpt-4o",
  "gitlens.views.branches.files.layout": "list",
  "gitlens.views.commits.files.layout": "list",
  "gitlens.views.commits.showBranchComparison": false,
  "gitlens.views.remotes.files.layout": "list",
  "go.formatTool": "gofmt",
  "go.lintFlags": [
    "--fast",
    "--config=${workspaceFolder}/.golangci.yml"
  ],
  "go.lintOnSave": "workspace",
  "go.lintTool": "golangci-lint",
  "go.playground": {
    "openbrowser": true,
    "run": true,
    "share": false
  },
  "gopls": {
    "formatting.gofumpt": true
  },
  "hediet.vscode-drawio.resizeImages": null,
  "highlight.regexes": {
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:FIX|FIXME|MUST|MUST_NOT|REMOVE_ME|BUG|UGLY|DEBUG|HACK)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "decorations": [
        {
          "backgroundColor": "#ff0000",
          "color": "#fff",
          "fontWeight": "bold",
          "overviewRulerColor": "#ff0000"
        },
        {
          "backgroundColor": "#ff0000",
          "color": "#fff"
        }
      ],
      "filterFileRegex": ".*(?<!(CHANGELOG.md|package.json))$"
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:IDEA)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "decorations": [
        {
          "backgroundColor": "#cc00cc",
          "color": "#1f1f1f",
          "fontWeight": "bold",
          "overviewRulerColor": "#cc00cc"
        },
        {
          "backgroundColor": "#cc00cc",
          "color": "#1f1f1f"
        }
      ],
      "filterFileRegex": ".*(?<!CHANGELOG.md)$"
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:REVIEW|OPTIMIZE|TSC)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "decorations": [
        {
          "backgroundColor": "#00ccff",
          "color": "#1f1f1f",
          "fontWeight": "bold",
          "overviewRulerColor": "#00ccff"
        },
        {
          "backgroundColor": "#00ccff",
          "color": "#1f1f1f"
        }
      ],
      "filterFileRegex": ".*(?<!CHANGELOG.md)$"
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:TODO|SHOULD|SHOULD_NOT|ASK_TO)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "decorations": [
        {
          "color": "#fff41a",
          "fontWeight": "bold",
          "overviewRulerColor": "#fff41a"
        },
        {
          "color": "#fff41a"
        }
      ],
      "filterFileRegex": ".*(?<!CHANGELOG.md)$"
    }
  },
  "javascript.preferences.quoteStyle": "single",
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown-mermaid.darkModeTheme": "base",
  "markdownlint.config": {
    "MD024": false,
    "MD036": false,
    "MD041": false
  },
  "material-icon-theme.files.associations": {
    ".envrc": "android",
    ".envrc.local": "android"
  },
  "material-icon-theme.files.color": "#90a4ae",
  "prettier.jsxSingleQuote": true,
  "prettier.printWidth": 200,
  "prettier.singleQuote": true,
  "remote.autoForwardPortsSource": "hybrid",
  "remote.WSL.debug": false,
  "remote.WSL.useShellEnvironment": true,
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.sendKeybindingsToShell": false,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.stickyScroll.maxLineCount": 5,
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules/*"
  ],
  "todo-tree.general.tags": [
    "TODO",
    "FIX",
    "FIXME",
    "MUST",
    "MUST_NOT",
    "SHOULD",
    "SHOULD_NOT",
    "ASK_TO",
    "REMOVE_ME"
  ],
  "todo-tree.highlights.enabled": false,
  "trailing-spaces.syntaxIgnore": [
    "markdown"
  ],
  "typescript.preferences.quoteStyle": "single",
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.useSystemClipboard": true,
  "window.confirmSaveUntitledWorkspace": false,
  "window.newWindowProfile": "Default",
  "workbench.colorCustomizations": {
    "activityBar.activeBorder": "#63c3dd",
    "editorStickyScroll.background": "#0a1a2ec1",
    "panel.border": "#bbbbbb0d",
    "panelTitle.activeBorder": "#63c3dd",
    "tab.activeBorder": "#63c3dd",
    "tab.selectedBackground": "#63c3dd15",
    "terminal.findMatchBackground": "#2b2700ec",
    "terminal.findMatchBorder": "#7b700dec",
    "editor.lineHighlightBackground": "#161926",
  },
  "workbench.colorTheme": "Material Theme Ocean",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.tree.enableStickyScroll": true,
  "yaml.format.enable": true,
  "dotenv.enableAutocloaking": false,
  "markdown-preview-github-styles.darkTheme": "light",
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "cSpell.enabled": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "diffEditor.hideUnchangedRegions.enabled": true,
  "editor.fontSize": 12,
  "window.zoomLevel": -1
}